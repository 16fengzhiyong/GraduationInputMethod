<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyKeyboard">

    </declare-styleable>
    <declare-styleable name="MyKeyboard_Key">
        <!-- {@link com.nuc.omeletteinputmethod.kernel.keyboard.KeyboardTextsTable.class}
             对应其中字符串，获取后解析  -->
        <attr name="altCode" format="string" />
        <!-- 更多按键，长按时显示
            {@link com.nuc.omeletteinputmethod.kernel.keyboard.KeyboardTextsTable.class}-->
        <attr name="moreKeys" format="string" />
        <!-- 需要在键盘上显示的键 -->
        <attr name="keySpec" format="string" />
        <!--  上下左右距离百分比   -->
        <attr name="padding_left" format="dimension" />
        <attr name="padding_right" format="dimension" />
        <attr name="padding_top" format="dimension" />
        <attr name="padding_bottom" format="dimension" />
        <!--   键的宽和高   -->
        <attr name="length" format="dimension" />
        <attr name="height" format="dimension" />
    </declare-styleable>
</resources>
